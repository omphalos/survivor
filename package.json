{
  "name": "survivor",
  "version": "0.0.2",
  "description": "A utility for finding the original location of changed text in a diff.",
  "main": "survivor.js",
  "scripts": {
    "test": "./node_modules/istanbul/lib/cli.js cover node_modules/nodeunit/bin/nodeunit -- ./tests.js",
    "build": "./node_modules/.bin/browserify -s survivor -r ./ | ./node_modules/.bin/uglifyjs > survivor.min.js; gzip -c survivor.min.js | wc -c"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/omphalos/survivor"
  },
  "keywords": [
    "diff"
  ],
  "author": "omphalos",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/omphalos/survivor/issues"
  },
  "homepage": "https://github.com/omphalos/survivor",
  "devDependencies": {
    "browserify": "^11.0.1",
    "googlediff": "^0.1.0",
    "istanbul": "^0.3.17",
    "nodemon": "^1.4.1",
    "nodeunit": "^0.9.1",
    "uglify-js": "^2.4.24"
  },
  "bin": "survivor.js",
  "dependencies": {}
}